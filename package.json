    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "npm run lint",
    "_clear": "rimraf dist/*",
    "lint": "eslint --fix src/**/*.ts",
